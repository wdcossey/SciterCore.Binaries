<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>SciterCore.Binaries</id>
    <version>$version$</version>
    <title>SciterCore.Binaries</title>
    <authors>William D Cossey</authors>
    <owners>wdcossey</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">GPL-3.0-or-later</license>
    <projectUrl>https://github.com/wdcossey/SciterCore</projectUrl>
    <description>SciterCore binaries for Windows, MacOS and Linux.</description>
    <copyright>© Sciter.</copyright>
    <tags>Sciter SciterCore WinForms WPF Gtk Xamarin</tags>
    <icon>images\icon.png</icon>
    <dependencies>
      <group targetFramework="net461"/>
      <group targetFramework="netstandard2.0"/>
      <group targetFramework="netcoreapp2.0"/>
      <group targetFramework="xamarinmac20"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\images\sciter_nuget_icon.png" target="images\icon.png" />
    <file src="lib\_._" target="lib/net461/_._" />
    <file src="lib\_._" target="lib/netstandard2.0/_._" />
    <file src="lib\_._" target="lib/netcoreapp2.0/_._" />
    <file src="lib\_._" target="lib/xamarinmac20/_._" />
    <file src="build\**\*.targets" target="build\net461" />
<!--    <file src="build\**\*.targets" target="build\netstandard2.0" />
    <file src="build\**\*.targets" target="build\netcoreapp2.0" />-->
    <file src="build\**\*.targets" target="build\xamarinmac20" />
    <file src="build\**\*.targets" target="buildMultiTargeting\net461" />
<!--    <file src="build\**\*.targets" target="buildMultiTargeting\netstandard2.0" />
    <file src="build\**\*.targets" target="buildMultiTargeting\netcoreapp2.0" />-->
    <file src="build\**\*.targets" target="buildMultiTargeting\xamarinmac20" />
    <file src="..\..\sciter-sdk\bin.lnx\x64\libsciter-gtk.so" target="runtimes\linux-x64\native\libsciter-gtk.so" />
    <file src="..\..\sciter-sdk\bin.osx\libsciter.dylib" target="runtimes\osx\native\sciter-osx-64.dylib" />
    <file src="..\..\sciter-sdk\bin.win\x32\sciter.dll" target="runtimes\win-x86\native\sciter.dll" />
    <file src="..\..\sciter-sdk\bin.win\x64\sciter.dll" target="runtimes\win-x64\native\sciter.dll" />
  </files>
</package>